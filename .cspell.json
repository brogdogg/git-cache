{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "**/bin/**",
    "**/obj/**",
    "**/packages/**",
    "**/.git/**",
    "**/.vs/**",
    "**/*.Designer.cs",
    "**/*.g.cs",
    "**/*.g.i.cs",
    "**/*.Generated.cs",
    "**/*.AssemblyInfo.cs",
    "**/*.nupkg",
    "**/*.dll",
    "**/*.exe",
    "**/*.pdb"
  ],
  "dictionaries": [
    "csharp",
    "dotnet",
    "softwareTerms"
  ],
  "words": [
    "ASPNETCORE",
    "async",
    "await",
    "bool",
    "brogdogg",
    "byte",
    "char",
    "csproj",
    "datetime",
    "DPDL",
    "enum",
    "gitcache",
    "Guid",
    "IConfiguration",
    "IDisposable",
    "IEnumerable",
    "ILFS",
    "IList",
    "ILogger",
    "IQueryable",
    "IReadOnlyCollection",
    "IReadOnlyList",
    "IServiceCollection",
    "ISet",
    "LINQ",
    "mstest",
    "namespace",
    "noninteractive",
    "nowarn",
    "nunit",
    "OIDIs",
    "readonly",
    "resx",
    "retval",
    "stringbuilder",
    "struct",
    "Task",
    "timespan",
    "ToString",
    "ulong",
    "ushort",
    "utc",
    "wwwroot"
  ],
  "ignoreWords": [
    "TODO",
    "FIXME"
  ],
  "enableFiletypes": [
    "csharp",
    "json",
    "xml",
    "markdown",
    "yaml"
  ]
}
