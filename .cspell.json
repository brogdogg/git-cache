{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "files": [
    "**/*.cs",
    "**/*.csproj",
    "**/*.json",
    "**/*.xml",
    "**/*.md",
    "**/*.yml",
    "**/*.yaml"
  ],
  "dictionaries": [
    "csharp",
    "dotnet",
    "softwareTerms"
  ],
  "words": [
    "ASPNETCORE",
    "async",
    "await",
    "bool",
    "brogdogg",
    "buildtransitive",
    "byte",
    "char",
    "contentfiles",
    "csproj",
    "datetime",
    "DPDL",
    "enum",
    "gitcache",
    "globaltool",
    "Guid",
    "IConfiguration",
    "IDisposable",
    "IEnumerable",
    "ILFS",
    "IList",
    "ILogger",
    "IQueryable",
    "IReadOnlyCollection",
    "IReadOnlyList",
    "IServiceCollection",
    "ISet",
    "LINQ",
    "mstest",
    "namespace",
    "Nito",
    "noninteractive",
    "nowarn",
    "nunit",
    "OIDIs",
    "readonly",
    "reportgenerator",
    "reporttypes",
    "resx",
    "retval",
    "stringbuilder",
    "struct",
    "targetdir",
    "Task",
    "timespan",
    "ToString",
    "ulong",
    "ushort",
    "utc",
    "wwwroot"
  ],
  "ignoreWords": [
    "TODO",
    "FIXME"
  ],
  "enableFiletypes": [
    "csharp",
    "json",
    "xml",
    "markdown",
    "yaml"
  ]
}